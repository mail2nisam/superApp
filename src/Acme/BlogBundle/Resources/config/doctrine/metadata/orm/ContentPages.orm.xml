<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContentPages" table="content_pages">
    <id name="contentid" type="integer" column="contentID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contentstatus" type="string" column="contentStatus" nullable="false"/>
    <field name="contentcreateddate" type="datetime" column="contentCreatedDate" nullable="false"/>
    <field name="contentisbasic" type="string" column="contentIsBasic" nullable="false"/>
    <field name="contentisforsubpages" type="string" column="contentIsForSubPages" nullable="true"/>
    <field name="contenturlkey" type="string" column="contentUrlKey" length="255" nullable="false"/>
    <field name="sectionid" type="integer" column="sectionID" nullable="false"/>
    <field name="sortorder" type="integer" column="sortOrder" nullable="false"/>
  </entity>
</doctrine-mapping>
