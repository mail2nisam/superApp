<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DealCampaigns" table="deal_campaigns">
    <id name="campaignid" type="integer" column="campaignID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dealid" type="integer" column="dealID" nullable="false"/>
    <field name="cityid" type="integer" column="cityID" nullable="false"/>
    <field name="dealtypekey" type="string" column="dealTypeKey" length="50" nullable="false"/>
    <field name="insertdate" type="datetime" column="insertDate" nullable="false"/>
    <field name="campaigndate" type="datetime" column="campaignDate" nullable="false"/>
  </entity>
</doctrine-mapping>
