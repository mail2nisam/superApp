      <generator strategy="IDENTITY"/>
    </id>
    <field name="affiliateid" type="string" column="affiliateID" length="25" nullable="true"/>
    <field name="affiliatereqid" type="integer" column="affiliateReqID" nullable="false"/>
    <field name="affiliateusername" type="string" column="affiliateUserName" length="100" nullable="true"/>
    <field name="affiliatepassword" type="string" column="affiliatePassword" length="50" nullable="true"/>
    <field name="affiliatefirstname" type="string" column="affiliateFirstName" length="100" nullable="true"/>
    <field name="affiliatelastname" type="string" column="affiliateLastName" length="100" nullable="true"/>
