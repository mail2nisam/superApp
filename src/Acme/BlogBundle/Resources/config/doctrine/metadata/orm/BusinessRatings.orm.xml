<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BusinessRatings" table="business_ratings">
    <id name="ratingid" type="bigint" column="ratingID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="businessid" type="integer" column="businessID" nullable="false"/>
    <field name="userid" type="bigint" column="userID" nullable="false"/>
    <field name="userrating" type="boolean" column="userRating" nullable="false"/>
    <field name="userrateddate" type="datetime" column="userRatedDate" nullable="false"/>
  </entity>
</doctrine-mapping>
