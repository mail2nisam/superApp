<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Campaigns" table="campaigns">
    <id name="campaignid" type="integer" column="campaignID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="thirdpartycampaignid" type="string" column="thirdPartyCampaignID" length="200" nullable="false"/>
    <field name="title" type="string" column="title" length="200" nullable="false"/>
    <field name="createddate" type="datetime" column="createdDate" nullable="false"/>
    <field name="scheduleddate" type="datetime" column="scheduledDate" nullable="false"/>
    <field name="content" type="text" column="content" nullable="false"/>
    <field name="opencount" type="integer" column="openCount" nullable="false"/>
    <field name="clickcount" type="integer" column="clickCount" nullable="false"/>
    <field name="bouncecount" type="integer" column="bounceCount" nullable="false"/>
    <field name="uniqueopenedcount" type="integer" column="uniqueOpenedCount" nullable="false"/>
    <field name="uniqueclickcount" type="integer" column="uniqueClickCount" nullable="false"/>
    <field name="unsubscribedcount" type="integer" column="unsubscribedCount" nullable="false"/>
    <field name="forwardscount" type="integer" column="forwardsCount" nullable="false"/>
    <field name="usercount" type="integer" column="userCount" nullable="false"/>
    <field name="businessid" type="integer" column="businessID" nullable="true"/>
  </entity>
</doctrine-mapping>
