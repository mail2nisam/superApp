<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CampaignAds" table="campaign_ads">
    <id name="campaignadid" type="integer" column="campaignAdID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adsize" type="string" column="adSize" length="9" nullable="false"/>
    <field name="campaignid" type="integer" column="campaignID" nullable="false"/>
    <field name="template" type="string" column="template" length="20" nullable="false"/>
    <field name="imagepath" type="text" column="imagePath" nullable="false"/>
    <field name="imageattr" type="text" column="imageAttr" nullable="false"/>
  </entity>
</doctrine-mapping>
