<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DealImpressions" table="deal_impressions">
    <id name="dealimpreid" type="integer" column="dealImpreID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dealid" type="integer" column="dealID" nullable="false"/>
    <field name="visitoridlist" type="text" column="visitorIDList" nullable="false"/>
    <field name="thumbviewcount" type="integer" column="thumbViewCount" nullable="false"/>
    <field name="visitdate" type="date" column="visitDate" nullable="false"/>
  </entity>
</doctrine-mapping>
