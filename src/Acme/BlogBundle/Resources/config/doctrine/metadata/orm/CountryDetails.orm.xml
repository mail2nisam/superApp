<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CountryDetails" table="country_details">
    <id name="countrydetailsid" type="integer" column="countryDetailsID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="countryid" type="integer" column="countryID" nullable="false"/>
    <field name="languageid" type="integer" column="languageID" nullable="false"/>
    <field name="countryname" type="string" column="countryName" length="100" nullable="false"/>
  </entity>
</doctrine-mapping>
