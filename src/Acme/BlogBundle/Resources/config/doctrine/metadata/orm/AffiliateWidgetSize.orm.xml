<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AffiliateWidgetSize" table="affiliate_widget_size">
    <id name="affiliatedealsizeid" type="integer" column="affiliateDealSizeID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="affiliatedealwidthpx" type="integer" column="affiliateDealWidthPx" nullable="false"/>
    <field name="affiliatedealheightpx" type="integer" column="affiliateDealHeightPx" nullable="false"/>
    <field name="widgetFormat" type="string" column="widget_format" nullable="false"/>
    <field name="widgetscrolling" type="string" column="widgetScrolling" nullable="false"/>
  </entity>
</doctrine-mapping>
