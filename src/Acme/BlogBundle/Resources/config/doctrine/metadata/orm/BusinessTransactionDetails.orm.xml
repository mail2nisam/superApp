<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BusinessTransactionDetails" table="business_transaction_details">
    <id name="transactiondetailsid" type="bigint" column="transactionDetailsID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="transactionid" type="bigint" column="transactionID" nullable="true"/>
    <field name="dealid" type="integer" column="dealID" nullable="true"/>
    <field name="orderid" type="bigint" column="orderID" nullable="true"/>
    <field name="transactiontotalamount" type="decimal" column="transactionTotalAmount" nullable="true"/>
  </entity>
</doctrine-mapping>
