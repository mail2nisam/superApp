<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="StateDetails" table="state_details">
    <id name="statedetailsid" type="integer" column="stateDetailsID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="stateid" type="integer" column="stateID" nullable="false"/>
    <field name="statename" type="string" column="stateName" length="100" nullable="false"/>
    <field name="languageid" type="integer" column="languageID" nullable="false"/>
    <field name="countryid" type="integer" column="countryID" nullable="false"/>
  </entity>
</doctrine-mapping>
