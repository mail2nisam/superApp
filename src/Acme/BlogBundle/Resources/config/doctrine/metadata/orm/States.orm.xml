<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="States" table="states">
    <id name="stateid" type="integer" column="stateID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="countryid" type="integer" column="countryID" nullable="false"/>
    <field name="stateshortname" type="string" column="stateShortName" length="10" nullable="false"/>
    <field name="statestatus" type="string" column="stateStatus" nullable="false"/>
  </entity>
</doctrine-mapping>
